{
  "expo": {
    "name": "Dart TV",
    "slug": "dart-tv",
    "version": "2.30.20",
    "orientation": "landscape",
    "icon": "./assets/DART-no-alpha.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/DART-graphene-splash.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
